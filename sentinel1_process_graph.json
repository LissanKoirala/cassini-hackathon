{
    "loadcollection1": {
        "process_id": "load_collection",
        "arguments": {
            "id": "SENTINEL1_GRD_MONTHLY_MOSAIC_IW",
            "spatial_extent": {},
            "temporal_extent": null,
            "bands": [
                "VV",
                "VH"
            ]
        }
    },
    "applydimension1": {
        "process_id": "apply_dimension",
        "arguments": {
            "data": {
                "from_node": "loadcollection1"
            },
            "dimension": "bands",
            "process": {
                "process_graph": {
                    "vh": {
                        "process_id": "array_element",
                        "arguments": {
                            "data": {
                                "from_parameter": "data"
                            },
                            "index": 1
                        }
                    },
                    "vv": {
                        "process_id": "array_element",
                        "arguments": {
                            "data": {
                                "from_parameter": "data"
                            },
                            "index": 0
                        }
                    },
                    "urban": {
                        "process_id": "multiply",
                        "arguments": {
                            "x": {
                                "from_node": "vh"
                            },
                            "y": 5.5
                        }
                    },
                    "vh_blue_channel": {
                        "process_id": "multiply",
                        "arguments": {
                            "x": {
                                "from_node": "vh"
                            },
                            "y": 8
                        }
                    },
                    "gt2": {
                        "process_id": "gt",
                        "arguments": {
                            "y": 0.5,
                            "x": {
                                "from_node": "urban"
                            }
                        }
                    },
                    "if_urban": {
                        "process_id": "if",
                        "arguments": {
                            "value": {
                                "from_node": "gt2"
                            },
                            "accept": 1,
                            "reject": 0
                        }
                    },
                    "arraycreate1": {
                        "process_id": "array_create",
                        "arguments": {
                            "data": [
                                {
                                    "from_node": "if_urban"
                                },
                                {
                                    "from_node": "vv"
                                },
                                {
                                    "from_node": "vh_blue_channel"
                                }
                            ]
                        },
                        "result": true
                    }
                }
            }
        }
    },
    "highlight": {
        "process_id": "highlight_compression",
        "arguments": {
            "data": {
                "from_node": "applydimension1"
            },
            "maxInput": 0.8,
            "clipInput": "0.9",
            "maxOutput": 1
        }
    },
    "saveresult1": {
        "process_id": "save_result",
        "arguments": {
            "data": {
                "from_node": "highlight"
            },
            "format": "PNG"
        },
        "result": true
    }
}
